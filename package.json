{
  "name": "@vmeta3/sdk",
  "version": "1.0.16",
  "description": "[Vmeta3] Tools for working with Vmeta3",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "all": "yarn clean && yarn build && yarn test && yarn lint:fix && yarn lint",
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist/ ./tsconfig.tsbuildinfo",
    "lint": "yarn lint:fix && yarn lint:check",
    "lint:check": "eslint . --max-warnings=0",
    "lint:fix": "yarn lint:check --fix",
    "pre-commit": "lint-staged",
    "test": "hardhat test",
    "test:coverage": "nyc hardhat test && nyc merge .nyc_output coverage.json",
    "autogen:docs": "typedoc --out docs src/index.ts"
  },
  "keywords": [
    "vmeta3",
    "ethereum",
    "sdk"
  ],
  "homepage": "https://github.com/VMETA3/js-sdk.git",
  "license": "MIT",
  "author": "Vmeta3 Labs",
  "repository": {
    "type": "git",
    "url": "https://github.com/VMETA3/js-sdk.git"
  },
  "devDependencies": {
    "@ethersproject/abstract-provider": "^5.6.1",
    "@ethersproject/abstract-signer": "^5.6.2",
    "@ethersproject/transactions": "^5.6.2",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "chai-as-promised": "^7.1.1",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.6.9",
    "hardhat": "^2.9.6",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.22.13"
  },
  "dependencies": {
    "@vmeta3/core-utils": "^1.0.2",
    "@vmeta3/contracts": "^1.0.6",
    "lodash": "^4.17.21",
    "merkletreejs": "^0.2.27",
    "rlp": "^2.2.7",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "ethers": "^5"
  }
}
